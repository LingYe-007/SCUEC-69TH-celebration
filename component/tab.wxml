<view class='tabbar'>
  <view wx:if='{{_auth >= item.auth}}' class='tabbar-item' wx:for='{{tabbarList}}' wx:key='{{item.pagePath}}'
    bindtap='handleItemTap' data-path='{{item.pagePath}}' data-idx='{{index}}'>
    <view class='tabbar-item-icon' id="{{item.id}}">
      <image src='{{activeIdx === index ? item.selectedIconPath : item.iconPath}}' class="icon" wx:if="{{item.show}}">
      </image>
    </view>
  </view>
  <view></view>
</view>